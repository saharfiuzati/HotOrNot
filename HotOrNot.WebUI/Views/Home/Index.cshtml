@model IEnumerable<HotOrNot.WebUI.Models.Picture>
@foreach (var item in Model)
{
           <div class="card" style="width: 100rem;">
        <div class="card-body">
            <h1 class="card-text display-2">We can add title for pictures here</h1>
            <img src="@Url.Content(item.FileName)" class="card-image-top img-thumbnail" style="width:350px ;height:350px" />
            <p class="card-text lead">We can add some description for pictures here</p>
            <a href="#" class="btn btn-primary">Like</a>
            <a href="#" class="btn btn-danger">Dislike</a>
        </div>
    </div>
}
